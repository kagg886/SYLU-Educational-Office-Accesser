<resources>
    <!-- Base application theme. -->
    <style name="Theme.掌上沈理青春版" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#7AEFB8</item>
        <item name="backgroundColor">@color/purple_200</item>

        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.掌上沈理青春版.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.掌上沈理青春版.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="Theme.掌上沈理青春版.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
</resources>